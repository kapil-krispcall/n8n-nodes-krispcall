{
	"name": "n8n-nodes-krispcall",
	"version": "0.1.3",
	"description": "An AI-Powered Modern Phone App custom node for n8n.",
	"keywords": [
		"n8n-community-node-package",
		"krispcall"
	],
	"license": "MIT",
	"homepage": "https://krispcall.com/",
	"author": {
		"name": "Kapil Bhandari",
		"email": "kapil@krispcallmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kapil-krispcall/n8n-nodes-krispcall.git"
	},
	"engines": {
		"node": ">=20.15"
	},
	"main": "index.js",
	"scripts": {
		"build": "npx rimraf dist && tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
		"test": "echo 'Error: no test specified' && exit 0"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/KrispcallApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Krispcall/Krispcall.node.js",
			"dist/nodes/Krispcall/KrispcallTrigger.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "~8.32.0",
		"eslint": "^8.57.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"gulp": "^5.0.0",
		"prettier": "^3.5.3",
		"typescript": "^5.8.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
